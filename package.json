{
  "name": "parcel-tracker",
  "version": "1.4.0",
  "description": "Parcel tracker can get parcel tracking data from multiple couriers",
  "main": "lib/parcel-tracker.js",
  "scripts": {
    "format": "prettier src/**/*.js --write",
    "lint": "eslint src --fix",
    "test": "mocha test/index.js",
    "build": "parcel build src/index.js --out-dir lib --out-file parcel-tracker.js --bundle-node-modules --no-source-maps --target node"
  },
  "mocha": {
    "timeout": 5000
  },
  "pre-commit": [
    "format",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicolas-goudry/parcel-tracker.git"
  },
  "author": "Nicolas Goudry <goudry.nicolas@gmail.com>",
  "bugs": {
    "url": "https://github.com/nicolas-goudry/parcel-tracker/issues"
  },
  "homepage": "https://github.com/nicolas-goudry/parcel-tracker#readme",
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/core": "7.7.7",
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/preset-env": "7.7.7",
    "babel-eslint": "10.0.3",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "mocha": "7.0.0",
    "parcel-bundler": "1.12.4",
    "pre-commit": "1.2.2",
    "prettier": "1.19.1",
    "standard": "14.3.1"
  },
  "dependencies": {
    "@hapi/joi": "16.1.8",
    "axios": "0.19.1",
    "cheerio": "1.0.0-rc.3",
    "he": "1.2.0",
    "lodash.get": "4.4.2",
    "lodash.omit": "4.5.0",
    "moment": "2.24.0",
    "moment-timezone": "0.5.27",
    "xml2js": "0.4.23"
  }
}
